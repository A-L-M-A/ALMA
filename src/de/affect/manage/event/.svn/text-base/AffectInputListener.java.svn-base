package de.affect.manage.event;

/** 
 * Classes that implement this interface and that are registered with
 * the <code>AffectInputListener</code> are notified if new input for
 * the affect generation is present.
 */
public interface AffectInputListener {
  public void updateInput(AffectInputEvent e);
}
