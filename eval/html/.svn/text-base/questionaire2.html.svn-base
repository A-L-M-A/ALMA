    <tr>
      <td width="10"><input name="knownemotion" value="Fear" type="checkbox" onclick="document.affect.addAnswer('User','Fear','yes')"></td><td id="emo" width="140">Sorge</td><td width="660">Ver&auml;rgert &uuml;ber die Chance auf ein unerw&uuml;nschtes Ereignis</td>
