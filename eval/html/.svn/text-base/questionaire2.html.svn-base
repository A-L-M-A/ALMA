    <tr>
      <td width="10"><input name="knownemotion" value="Reproach" type="checkbox" onclick="document.affect.addAnswer('User','Reproach','yes')"></td><td id="emo" width="140">Verachtung</td><td width="660">Tadelnsw&uuml;rdige Handlung eines anderen f&uuml;r schlecht befinden</td>
