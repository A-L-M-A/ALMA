    <tr>
      <td width="10"><input name="knownemotion" value="Admiration" type="checkbox" onclick="document.affect.addAnswer('User','Admiration','yes')"></td><td id="emo" width="140">Bewunderung</td><td width="660">Lobenswerte Handlung eines anderen f&uuml;r gut befinden</td>
