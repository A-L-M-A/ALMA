    <tr>
      <td width="10"><input name="knownemotion" value="Gloating" type="checkbox" onclick="document.affect.addAnswer('User','Gloating','yes')"></td><td id="emo" width="140">Schadenfreude</td><td width="660">Erfreut &uuml;ber ein Ereignis, das unerw&uuml;nscht f&uuml;r jemand anderen ist</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="SorryFor" type="checkbox" onclick="document.affect.addAnswer('User','SorryFor','yes')"></td><td id="emo" width="140">Mitleid</td><td width="660">Ver&auml;rgert &uuml;ber ein Ereignis, das unerw&uuml;nscht f&uuml;r jemand anderen ist</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Resentment" type="checkbox" onclick="document.affect.addAnswer('User','Resentment','yes')"></td><td id="emo" width="140">Neid</td><td width="660">Ver&auml;rgert &uuml;ber ein Ereignis, das w&uuml;nschenswert f&uuml;r jemand anderen ist</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Hope" type="checkbox" onclick="document.affect.addAnswer('User','Hope','yes')"></td><td id="emo" width="140">Hoffnung</td><td width="660">Erfreut &uuml;ber die Chance auf ein w&uuml;nschenswertes Ereignis</td>
