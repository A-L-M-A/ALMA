    <tr>
      <td width="10"><input name="knownemotion" value="Disliking" type="checkbox" onclick="document.affect.addAnswer('User','Disliking','yes')"></td><td id="emo" width="140">Ablehnung (von Dingen)</td><td width="660">Ablehnung eines unattraktiven Gegenstands</td>
