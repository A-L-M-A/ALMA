    <tr>
      <td width="10"><input name="knownemotion" value="Disappointment" type="checkbox" onclick="document.affect.addAnswer('User','Disappointment','yes')"></td><td id="emo" width="140">Entt&auml;uschung</td><td width="660">Ver&auml;rgert &uuml;ber die Widerufung des Eintretens eines m&ouml;glichen Ereignisses</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Relief" type="checkbox" onclick="document.affect.addAnswer('User','Relief','yes')"></td><td id="emo" width="140">Erleichterung</td><td width="660">Erfreut &uuml;ber die Widerufung des Eintretens eines m&ouml;glichen unerw&uuml;nschten Ereignisses</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="FearsConfirmed" type="checkbox" onclick="document.affect.addAnswer('User','FearsConfirmed','yes')"></td><td id="emo" width="140">Angst</td><td width="660">Ver&auml;rgert &uuml;ber die Best&auml;tigung des Eintretens eines m&ouml;glichen unerw&uuml;nschten Ereignisses</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Pride" type="checkbox" onclick="document.affect.addAnswer('User','Pride','yes')"></td><td id="emo" width="140">Stolz</td><td width="660">Eigene lobenswerte Handlung f&uuml;r gut befinden</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Shame" type="checkbox" onclick="document.affect.addAnswer('User','Shame','yes')"></td><td id="emo" width="140">Scham</td><td width="660">Eigene tadelnsw&uuml;rdige Handlung f&uuml;r schlecht befinden</td>
