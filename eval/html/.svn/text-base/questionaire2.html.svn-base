    <tr>
      <td width="10"><input name="knownemotion" value="Gratification" type="checkbox" onclick="document.affect.addAnswer('User','Gratification','yes')"></td><td id="emo" width="140">Dankbarkeit</td><td width="660">Bewunderung + Freude</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Anger" type="checkbox" onclick="document.affect.addAnswer('User','Anger','yes')"></td><td id="emo" width="140">Wut</td><td width="660">Verachtung + Missfallen</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Gratitude" type="checkbox" onclick="document.affect.addAnswer('User','Gratitude','yes')"></td><td id="emo" width="140">Genugtuung</td><td width="660">Stolz + Freude</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Remorse" type="checkbox" onclick="document.affect.addAnswer('User','Remorse','yes')"></td><td id="emo" width="140">Reue</td><td width="660">Scham + Missfallen</td>
    </tr>
    <tr>
      <td width="10"><input name="knownemotion" value="Liking" type="checkbox" onclick="document.affect.addAnswer('User','Liking','yes')"></td><td id="emo" width="140">Gefallen (an Dingen)</td><td width="660">Gefallen eines attraktiven Gegenstands</td>
