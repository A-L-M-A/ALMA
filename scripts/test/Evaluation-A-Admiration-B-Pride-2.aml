<?xml version="1.0" encoding="ISO-8859-1"?>
<aml:AffectScript xmlns:aml="xml.affect.de">
	<aml:Context>Bruno und Anne sind Nachbarn und verstehen sich prima</aml:Context>
	<aml:Item time="0">
		<aml:ResetCharacter name="Anne"/>
	</aml:Item>
	<aml:Item time="0">
		<aml:ResetCharacter name="Bruno"/>
	</aml:Item>
	<aml:Item time="1000">
		<aml:AffectInput>
			<aml:Character name="Anne"/>
			<aml:Act addressee="Bruno" listener="" type="Admire" intensity="0.50" elicitor="ABC"/>
		</aml:AffectInput>
		<aml:Context>Anne: Bruno, Du machst einen fantastischen Kaffee [signal]</aml:Context>
	</aml:Item>
</aml:AffectScript>
